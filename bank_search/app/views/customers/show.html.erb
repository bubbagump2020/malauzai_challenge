<% provide :head_tags do %>
    <script>
    document.addEventListener("DOMContentLoaded", function(){
        initMap(<%=@search.latitude%>, <%=@search.longitude%>)
    });
    </script>
<% end %>

<!DOCTYPE html>
<head>
</head>
    <style>
        #map{
            height: 800px;
            width: 800px;
        }
    </style>
<body>
  <h2>Customer Information</h2>
  <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 30.4284750, lng: -97.7550500},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/place/findplacefromtext/<%= @customer.response %>?input=<%= @customer.name %>&inputtype=textquery&fields=<%= @customer.bank_type %>&language=<%= @customer.language%>&locationbias=point:<%= @search.latitude %>,<%= @search.longitude %>&key=AIzaSyBAnEpGMdG4aeb-chrxedvGa74BYhpY2DI"
    async defer></script>
  <div>
    <strong><a href="/">New Search</a></strong>
  </div>
  <div>
    <strong><a href="/customer_searches">Search Index</a></strong>
  </div>
<body>
</html>