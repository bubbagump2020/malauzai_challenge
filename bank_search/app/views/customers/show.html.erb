<% provide :head_tags do %>
    <script>
    document.addEventListener("DOMContentLoaded", function(){
        initMap(<%=@search.latitude%>, <%=@search.longitude%>)
    });
    </script>
<% end %>

<!DOCTYPE html>
<head>
</head>
    <style>
        #map{
            height: 800px;
            width: 800px;
        }
    </style>
<body>
  <h2>Customer Information</h2>
  <div id="map"></div>
   <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: <%=@search.latitude%>, lng: <%=@search.longitude%>},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAnEpGMdG4aeb-chrxedvGa74BYhpY2DI&callback=initMap"
    async defer></script>
  <div>
    <strong><a href="/">New Search</a></strong>
  </div>
