<!DOCTYPE html>
<head>
</head>
    <style>
        #map{
            height: 800px;
            width: 800px;
        }
    </style>
<body>
  <h1>Customer Show</h1>
  <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 30.4284750, lng: -97.7550500},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/place/findplacefromtext/<%= @customer.response %>?input=<%= @customer.name %>&inputtype=textquery&fields=<%= @customer.bank_type %>&language=<%= @customer.language%>&locationbias=point:<%= @search.latitude %>,<%= @search.longitude %>&key=AIzaSyBAnEpGMdG4aeb-chrxedvGa74BYhpY2DI"
    async defer></script>
  <div>
    <%= @customer.response %>
  </div>
  <div>
    <a href="/">New Search</a>
  </div>
  <div>
    <a href="/customer_searches">Search Index</a>
  </div>
<body>
</html>